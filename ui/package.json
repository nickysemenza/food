{
  "name": "gourd-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.1.1",
    "@sentry/browser": "6.2.4",
    "@types/jest": "26.0.22",
    "@types/jwt-decode": "3.1.0",
    "@types/node": "14.14.37",
