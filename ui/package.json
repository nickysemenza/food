    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "@types/react-select": "3.1.2",
    "@types/react-table": "7.0.29",
    "blurhash": "1.1.3",
    "core-js": "3.10.0",
    "dayjs": "1.10.4",
    "immutability-helper": "3.1.1",
    "jwt-decode": "3.1.2",
    "parse-duration": "1.0.0",
    "pluralize": "8.0.0",
    "query-string": "7.0.0",
    "react": "17.0.2",
    "react-blurhash": "0.1.3",
    "react-cookie": "4.0.3",
    "react-dnd": "11.1.3",
    "react-dnd-html5-backend": "11.1.3",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-google-login": "5.2.2",
    "react-hotkeys-hook": "3.3.0",
    "react-json-view": "1.21.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "4.0.2",
    "react-table": "7.6.3",
    "react-toastify": "7.0.3",
    "redoc": "2.0.0-rc.50",
    "restful-react": "15.4.0",
    "gourd_rs": "file:../rust/w/pkg",
    "styled-components": "5.2.2",
    "typescript": "4.2.3",
    "universal-cookie": "4.0.4",
    "wasm-loader": "1.3.0"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "prebuild": "run-s build:tailwind",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "x": "x",
    "start:react": "REACT_APP_API_URL=http://localhost:4242 craco -r @cypress/instrument-cra start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "generate-fetcher": "restful-react import --file ../api/openapi.yaml --output src/api/openapi-hooks/api.tsx",
    "postgenerate-fetcher": "prettier src/api/openapi-hooks/api.tsx --write",
    "coverage:unit": "craco test --watchAll=false --coverage --coverageDirectory=coverage/unit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "start:integration": "BROWSER=none PORT=3001 craco -r @cypress/instrument-cra start",
    "cy:run:integration": "cypress run --record --key 265312e3-7b8c-4031-ad0a-5f825b95cfce",
    "test:integration": "start-server-and-test start:integration http://localhost:3001 cy:run:integration"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.4",
    "@openapitools/openapi-generator-cli": "2.2.4",
    "@cypress/instrument-cra": "1.4.0",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.1",
    "autoprefixer": "10.2.5",
    "chokidar-cli": "2.1.0",
    "cypress": "6.8.0",
    "cypress-terminal-report": "3.0.2",
    "npm-run-all": "4.1.5",
    "ibm-openapi-validator": "0.39.0",
    "postcss": "8.2.9",
    "prettier": "2.2.1",
    "redoc-cli": "0.10.4",
    "start-server-and-test": "1.12.1",
    "tailwindcss": "2.0.4"
  }
}
